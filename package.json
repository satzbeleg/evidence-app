{
  "name": "evidence-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "css-build": "node-sass --omit-source-map-url sass/darktheme.scss public/css/darktheme.css && node-sass --omit-source-map-url sass/lighttheme.scss public/css/lighttheme.css",
    "new-icons": "npx vue-pwa-asset-generator -a src/assets/logo.png -o public/img/icons --no-manifest && mv public/img/icons/favicon.ico public/favicon.ico"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@tensorflow/tfjs": "^3.9.0",
    "@vue/preload-webpack-plugin": "^1.1.2",
    "axios": "^0.21.4",
    "bulma": "^0.9.0",
    "bulma-pageloader": "^0.3.0",
    "bulma-slider": "^2.0.4",
    "bulma-switch": "^2.0.0",
    "bulma-toast": "^2.0.3",
    "bwsample": "^0.1.3",
    "core-js": "^3.10.0",
    "histpdf": "0.1.0",
    "js-cookie": "^2.2.1",
    "messageformat": "^2.3.0",
    "register-service-worker": "^1.7.1",
    "uuid": "^8.3.2",
    "vue": "^3.0.10",
    "vue-fit2box": "1.0.0",
    "vue-i18n": "^9.0.0-beta.2",
    "vue-router": "^4.0.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-alpha.0",
    "glob-parent": "^5.1.2",
    "jpeg-js": "^0.4.0",
    "minimist": "^0.2.1",
    "node-sass": "^7.0.0",
    "sass-loader": "^10",
    "trim-newlines": "^3.0.1",
    "vue-pwa-asset-generator": "^1.6.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
